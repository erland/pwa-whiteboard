{
  "name": "pwa-whiteboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "deploy": "gh-pages -d dist",
    "typecheck": "tsc -p tsconfig.json",
    "typecheck:all": "tsc -p tsconfig.json && tsc -p shared/tsconfig.json && tsc -p workers/collab/tsconfig.json",
    "typecheck:shared": "tsc -p shared/tsconfig.json",
    "typecheck:worker": "tsc -p workers/collab/tsconfig.json",
    "worker:dev": "cd workers/collab && npx wrangler dev",
    "worker:deploy": "cd workers/collab && npx wrangler deploy"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "@supabase/supabase-js": "^2.49.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "gh-pages": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^7.2.6"
  }
}
